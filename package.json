{
  "name": "xeptrix",
  "version": "0.1.4",
  "displayName": "Xeptrix - Document Format Converter",
  "description": "Document Format Converter for Javascript written in TypeScript",
  "keywords": [
    "format",
    "html",
    "parser",
    "rtf",
    "convert",
    "node",
    "javascript",
    "typescript"
  ],
  "author": "Peter Nicholls <halos_augury_06@icloud.com>",
  "license": "AGPLv3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peternicholls/xeptrix.git"
  },
  "homepage": "https://github.com/peternicholls/xeptrix#readme",
  "bugs": {
    "url": "https://github.com/peternicholls/xeptrix/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jest.config.json"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@jest/globals": "^29.5.0",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.12",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "chokidar": "^3.5.3",
    "copy": "^0.3.2",
    "eslint": "^8.39.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.5.0",
    "jest-puppeteer": "^8.0.6",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.6",
    "typedoc-plugin-markdown": "^3.15.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@types/jsdom": "^21.1.1",
    "ascii-table3": "^0.8.2",
    "jsdom": "^21.1.1",
    "node-fetch": "^3.3.1",
    "sharp": "^0.32.0"
  }
}
